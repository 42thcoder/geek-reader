<div class="title">
    <%= @entry.get("title")%>
</div>
<div class="feed title">
    <%= @entry.get("author")%>
</div>
<div class="detail">
    <h3><%= @entry.get("title") %></h3>
    <span class="glyphicon glyphicon-minus close"></span>
    <div class="body">
        <% if @entry.has("content") and @entry.get("content") isnt "" : %>
            <%- @entry.get("content")%>
        <% else : %>
            <%- @entry.get("summary")%>
        <% end %>
    </div>
</div>