<div class="tab-pane active" id="settings_feeds">
    <ol class="feeds">
        <% for feed in @feeds: %>
        <li data-id="<%=feed.id%>"><%= feed.get("title") %>
            <i class="pull-right glyphicon glyphicon-edit edit"></i>
            <i class="pull-right glyphicon glyphicon-remove delete"></i>
        </li>
        <% end %>
    </ol>
</div>